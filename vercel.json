{
  "buildCommand": "echo 'No build needed - static site'",
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/",
      "dest": "/fashion/index.html"
    },
    {
      "src": "/fashion",
      "dest": "/fashion/index.html"
    },
    {
      "src": "/fashion/(.*)",
      "dest": "/fashion/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
